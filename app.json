{
  "expo": {
    "name": "Romatem",
    "slug": "native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appstore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/romatem-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-calendar",
    {
        "calendarPermission": "Allow $(PRODUCT_NAME) to access your calendar",
        "remindersPermission": "Allow $(PRODUCT_NAME) to access your reminders"
      }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "Bu uygulama etkinlik biletlerindeki barkodları tarımak için kamerayı kullanır.",
        "NSRemindersUsageDescription": "Uygulama hatırlatıcılarınıza erişmek için izin verin."
      },
      "bundleIdentifier": "iosv1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appstore.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "metaworldbilisim.com"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "8f3aec24-1442-44a9-9fe5-647de47681de"
      }
    }
  }
}
